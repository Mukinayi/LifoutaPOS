<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.exact_it_dev.lifoutapos.transfert.ResultTransfertVerification"
    android:padding="5dp"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="25dp"
            android:text="@string/transfertdetails"
            android:textSize="32sp" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView9"
            android:layout_marginLeft="14dp"
            android:layout_marginStart="14dp"
            android:layout_marginTop="50dp"
            android:text="@string/prenom"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/labelPenom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView10"
            android:layout_marginLeft="24dp"
            android:layout_marginStart="24dp"
            android:layout_toEndOf="@+id/textView10"
            android:layout_toRightOf="@+id/textView10"
            android:text="TextView"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/labelNom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView10"
            android:layout_alignStart="@+id/textView10"
            android:layout_below="@+id/textView10"
            android:layout_marginTop="11dp"
            android:text="@string/nom"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/LabelNom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/labelNom"
            android:layout_alignLeft="@+id/labelPenom"
            android:layout_alignStart="@+id/labelPenom"
            android:text="TextView"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/labelNom"
            android:layout_alignStart="@+id/labelNom"
            android:layout_below="@+id/labelNom"
            android:layout_marginTop="14dp"
            android:text="@string/phone"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/labelPortable"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView14"
            android:layout_alignLeft="@+id/LabelNom"
            android:layout_alignStart="@+id/LabelNom"
            android:text="TextView"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView14"
            android:layout_alignStart="@+id/textView14"
            android:layout_below="@+id/textView14"
            android:layout_marginTop="11dp"
            android:text="@string/montant"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/labelMontant"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView16"
            android:layout_alignLeft="@+id/labelPortable"
            android:layout_alignStart="@+id/labelPortable"
            android:text="TextView"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView16"
            android:layout_alignStart="@+id/textView16"
            android:layout_below="@+id/textView16"
            android:layout_marginTop="14dp"
            android:text="@string/transfertstate"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/labelEtat"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView18"
            android:layout_alignLeft="@+id/labelMontant"
            android:layout_alignStart="@+id/labelMontant"
            android:text="TextView"
            android:textSize="18sp" />

        <Button
            android:id="@+id/btncontinuer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView18"
            android:layout_marginTop="52dp"
            android:background="@drawable/button"
            android:text="@string/continuer" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView18"
            android:layout_alignStart="@+id/textView18"
            android:layout_below="@+id/textView18"
            android:layout_marginTop="16dp"
            android:text="@string/feeswithdraw"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/labelFees"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView20"
            android:layout_alignBottom="@+id/textView20"
            android:layout_marginLeft="15dp"
            android:layout_marginStart="15dp"
            android:layout_toEndOf="@+id/labelEtat"
            android:layout_toRightOf="@+id/labelEtat"
            android:text="TextView"
            android:textSize="18sp" />

        <Button
            android:id="@+id/buttonAnnuler"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_below="@+id/btncontinuer"
            android:layout_centerHorizontal="true"
            android:background="@drawable/button"
            android:text="Annuler" />
    </RelativeLayout>

</LinearLayout>
